{
  "name": "story-genius-official-production",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -- turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@playwright/test": "^1.32.3",
    "@storybook/react": "^7.0.5",
    "babel-loader": "^9.1.2",
    "dotenv-cli": "^7.2.1",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.7",
    "supabase": "^1.50.4",
    "turbo": "^1.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": "18.16.0"
  },
  "dependencies": {
    "detect-port": "^1.5.1",
    "next": "13.3.0",
    "playwright": "^1.32.3"
  },
  "packageManager": "pnpm@8.1.1"
}
