{
  "name": "story-genius-official-production",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest",
    "next": "13.3.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "next": "13.3.0"
  },
  "packageManager": "pnpm@8.1.1"
}